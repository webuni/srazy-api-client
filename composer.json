{
    "name": "webuni/srazy-api-client",
    "description": "The missing API for http://srazy.info",
    "keywords": ["srazy.info", "webuni", "api"],
    "type": "library",
    "require": {
        "php": "^5.5|^7.0",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/dom-crawler": "^3.1@dev",
        "symfony/css-selector": "^3.0",
        "doctrine/collections": "^1.3",
        "ocramius/proxy-manager": "^1.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.0",
        "phpunit/phpunit": "^4.8|^5.2",
        "fabpot/php-cs-fixer": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "Webuni\\Srazy\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webuni\\Srazy\\Tests\\": "tests"
        }
    },
